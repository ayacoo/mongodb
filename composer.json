{
	"name": "ayacoo/mongodb",
	"type": "typo3-cms-extension",
	"version": "2.1.0",
	"description": "TYPO3 Mongo DB cache backend",
	"authors": [
		{
			"name": "Guido Schmechel",
			"role": "Developer"
		}
	],
	"homepage": "https://www.ayacoo.de/",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": "7.4",
		"ext-mongodb": "*",
		"mongodb/mongodb": "^1.9",
		"typo3/cms-core": "^11.0"
	},
	"autoload": {
		"psr-4": {
			"Ayacoo\\MongoDB\\": "Classes/"
		}
	},
	"replace": {
		"typo3-ter/mongodb": "self.version"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "mongodb",
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
